{
  "name": "rinter",
  "version": "2.0.0",
  "description": "Minimalist state container based on reactive extensions",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "repository": "https://github.com/dfernandez79/rinter.git",
  "author": "Diego Fernandez <diegof79@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "state",
    "predictable",
    "reducer",
    "rxjs",
    "flux",
    "elm"
  ],
  "peerDependencies": {
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "eslint": "^5.9.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.7.1",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3"
  },
  "scripts": {
    "build": "npm-run-all build:lib build:esm",
    "build:lib": "babel src -d lib --env-name lib",
    "build:esm": "babel src -d lib-esm --env-name esm",
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rimraf lib lib-esm *.tgz coverage *.log",
    "lint": "eslint src",
    "release": "npm-run-all clean lint build release:np",
    "release:np": "np"
  }
}
