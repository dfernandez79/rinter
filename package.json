{
  "name": "rinter",
  "version": "2.0.0-1",
  "description": "Minimalist state container based on reactive extensions",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "repository": "https://github.com/rinterjs/rinter.git",
  "author": "Diego Fernandez <diegof79@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "peerDependencies": {
    "lodash": ">=2.4.0",
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "ava": "0.25.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "scripts": {
    "build": "npm-run-all build:node build:esm",
    "build:node": "tsc",
    "build:esm": "tsc -p tsconfig-esm.json",
    "test": "npm-run-all build:node test:run",
    "test:run": "ava",
    "coverage": "npm-run-all build:node coverage:run",
    "coverage:run": "nyc --reporter=html ava",
    "clean": "rimraf lib lib-esm *.tgz coverage .nyc_output *.log",
    "lint": "tslint -p tsconfig.json"
  },
  "ava": {
    "files": [
      "lib/**/*.test.js"
    ]
  }
}
